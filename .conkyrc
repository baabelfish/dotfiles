background        no
out_to_console    yes
out_to_x          no
update_interval   1
total_run_times   0
short_units       yes
pad_percents      3

override_utf8_locale yes

TEXT
,[
  {"full_text": "${exec ~/.xutils/current_window_title.sh}","color": "\#aaaaaa"},
