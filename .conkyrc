  {"full_text": "≈ ${if_up wlan0}${wireless_essid wlan0} [${wireless_link_qual_perc wlan0}%]${else}[${addr eth0}]${endif}","color": "\#aaaaaa"},
  {"full_text": "◢ ","color": "\#00bb00"},
  {"full_text": "❤ $acpitemp°C","color": "\#aaaaaa","align": "right"},
  {"full_text": "◢ ","color": "\#00bb00"},
  {"full_text": "≣ [$membar]", "color":"\#aaaaaa"},
  {"full_text": "◢ ","color": "\#00bb00"},
  {"full_text": "${time %H:%M:%S}","color": "\#aaaaaa"},
  {"full_text": "◢ ","color": "\#00bb00"},
  {"full_text": "${time %a %d.%m.%Y}","color": "\#aaaaaa"},
  {"full_text": "◢ ","color": "\#00bb00"}
]
