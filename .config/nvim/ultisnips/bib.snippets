snippet cite "" !wb
@${1:article}{${2:refname},
    author={${3}},
    publisher={${4}},
    title={${5}},
    year={${6}},
}
endsnippet

snippet misc "" !wb
@misc{$1,
    author={$2},
    note=\{Viitattu ${3:`date +%d.%m.%Y`}\},
    publisher={$4},
    title={$5},
    url={$6},
    year={$7},
}
endsnippet
